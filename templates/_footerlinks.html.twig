<section class="mb-4">
  {% for personalInfos in aboutMe %}
  <a
    class="btn btn-outline-light btn-floating m-1"
    href="{{ personalInfos.linkedinLink }}"
    role="button"
  >
    <i class="bi bi-linkedin"></i>
  </a>

  <a
    class="btn btn-outline-light btn-floating m-1"
    href="{{ personalInfos.githubLink }}"
    role="button"
  >
    <i class="bi bi-github"></i>
  </a>
  {% endfor %}
</section>
